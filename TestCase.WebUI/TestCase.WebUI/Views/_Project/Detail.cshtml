@using TestCase.Infrastructure.Data;
@model  Project;
@{
    var project = Model;
}
<div class="row">
    <div id="breadcrumb" class="col-md-12">
        <ol class="breadcrumb">
            <li><a  href='@Url.Action("Privacy","Home")'>首页</a></li>
            <li><a  href='@Url.Action("Index","_Project")'>项目</a></li>
            <li><a href="#">详情</a></li>
        </ol>
    </div>
</div>
<div class="row">
    <div class="col-xs-12">
        <div class="box ">
            <div class="box-header">
                <div class="box-name">
                    <i class="fa fa-linux"></i>
                    <span>项目详情</span>
                </div>
                <div class="no-move"></div>
            </div>
            <div class="box-content">
                <form class="col-xs-12 " id="update"  method="post" action="@Url.Action("Update","_Project")">
                    <div class="form-group col-xs-10">
                        <label class="control-label">编号</label>
                        <input class="form-control" name="proid" value="@project.Proid" readonly />
                        <label class="control-label">标题</label>
                        <input type="text" class="form-control" name="proname" value="@project.Proname" />
                    </div>
                    <div class="form-group col-xs-10">
                        <label class="control-label">项目说明</label>
                        <textarea class="form-control" name="stroage" style="height:200px;">@project.Stroage</textarea>
                    </div>
                </form>
                <div class="clearfix"></div>
                <div class="col-sm-4 col-sm-offset-3">
                    <br />
                    <div class="btn-group-justified">
                        <a class="btn btn-default btn-link" href='@Url.Action("Index","_Project")' ><span><i class="fa fa-times-circle-o"></i></span> 返回</a>
                        <a class="btn btn-success btn-link  admin" style="color:dimgray;display:none;" onclick="toUpdate()"><span><i class="fa fa-check-circle"></i></span> 提交</a>
                    </div>
                </div>
            </div>
        </div>
        
    </div>
</div>
<script src="~/base.js"></script>